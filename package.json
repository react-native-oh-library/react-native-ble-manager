{
  "name": "@react-native-oh-tpl/react-native-ble-manager",
  "version": "11.5.2-0.0.7",
  "description": "A BLE module for react native.",
  "repository": {
    "type": "git",
    "url": "https://github.com/innoveit/react-native-ble-manager.git"
  },
  "bugs": {
    "url": "https://github.com/innoveit/react-native-ble-manager/issues"
  },
  "keywords": [
    "react-native",
    "android",
    "ios",
    "ble",
    "bluetooth",
    "low energy"
  ],
  "license": "Apache-2.0",
  "author": {
    "name": "Innove",
    "url": "https://github.com/innoveit"
  },
  "main": "dist/cjs/index.js",
  "types": "dist/esm/index.d.ts",
  "module": "dist/esm/index.js",
  "react-native": "src/index.ts",
  "files": [
    "/dist",
    "/src",
    "/android",
    "!/android/build",
    "/ios",
    "/*.podspec",
    "/harmony"
  ],
  "harmony": {
    "alias": "react-native-ble-manager",
    "codegenConfig": {
      "specPaths": [
        "./src"
      ]
    }
  },
  "scripts": {
    "version": "git add -A",
    "clean": "rimraf dist",
    "type:check": "tsc -p tsconfig.json --noEmit",
    "prebuild": "npm run clean",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "build:esm": "tsc --outDir dist/esm",
    "build": "npm run build:esm && npm run build:cjs",
    "watch": "npm run build --watch",
    "prepare": "npm run build",
    "preversion": "npm run build",
    "postversion": "git push --follow-tags"
  },
  "peerDependencies": {
    "react-native": ">=0.60.0",
    "react-native-create-thumbnail": "2.0.0",
    "react-native-ble-manager":"11.5.2"
  },
  "devDependencies": {
    "react-native": "0.73.6",
    "rimraf": "4.1.2",
    "typescript": "4.9.5"
  },
  "dependencies": {
    "react-native-svg": "^15.2.0",
    "react-native-typing-animation": "^0.1.7",
    "react-native-video-cache": "^2.7.4"
  }
}
